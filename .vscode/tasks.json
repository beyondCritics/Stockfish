{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "mingw build",
            "type": "shell",
            "command": "make",
            "args": [
                "-C",
                "\"${workspaceFolder}\\src\"",
                "build",
                "ARCH=x86-64-modern",
                "COMP=mingw",
                "-j",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "mingw profile build",
            "type": "shell",
            "command": "make",
            "args": [
                "-C",
                "\"${workspaceFolder}\\src\"",
                "profile-build",
                "ARCH=x86-64-modern",
                "COMP=mingw",
                "-j",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "mingw profile use",
            "type": "shell",
            "command": "make",
            "args": [
                "-C",
                "\"${workspaceFolder}\\src\"",
                "gcc-profile-use",
                "ARCH=x86-64-modern", 
                "COMP=mingw",
                "-j",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
}   